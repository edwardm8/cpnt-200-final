{
	"name": "directus-extension-directus-operation-lodash",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-operation"
	],
	"type": "module",
	"directus:extension": {
		"type": "operation",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"source": {
			"app": "src/app.js",
			"api": "src/api.js"
		},
		"host": "^10.2.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.2.0",
		"vue": "^3.3.10"
	},
	"dependencies": {
		"lodash": "^4.17.21"
	}
}
